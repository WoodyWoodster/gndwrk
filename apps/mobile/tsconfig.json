{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@gndwrk/types": [
        "../../packages/types/src"
      ],
      "@gndwrk/ui": [
        "../../packages/ui/src"
      ],
      "@gndwrk/convex/*": [
        "../../packages/convex/convex/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}